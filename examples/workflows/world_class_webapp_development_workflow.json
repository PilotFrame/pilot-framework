  "execution_spec": {
    "description": "Enterprise-ready mixed workflow orchestrating all personas for world-class web application delivery. Defines parallel, sequential, and cyclic flow patterns with deterministic metrics, fallbacks, and explicit post-cycle handoff.",
    "flow_pattern": "mixed",
    "parallel_details": {
      "parallel_steps": [
        "step_1_architecture_design",
        "step_2_creative_design"
      ],
      "merge_strategy": "all"
    },
    "cycle_details": {
      "cycle_steps": [
        "step_6_accessibility_qa",
        "step_7_performance_audit"
      ],
      "exit_condition": "accessibility_score >= 0.9 AND lighthouse_score >= 90 AND confidence_score >= 0.9",
      "max_iterations": 5
    },
    "cycle_metrics": {
      "accessibility_score": {
        "source": "accessibility_qa_reviewer.accessibility_qa_report.metrics.accessibility_score",
        "target_threshold": 0.9
      },
      "lighthouse_score": {
        "source": "performance_auditor_persona.performance_audit_report.lighthouse.score",
        "target_threshold": 90
      },
      "confidence_score": {
        "aggregation": "min",
        "sources": [
          "accessibility_qa_reviewer.accessibility_qa_report.metadata.confidence_score",
          "performance_auditor_persona.performance_audit_report.metadata.confidence_score"
        ]
      }
    },
    "confidence_strategy": {
      "scope": "qa_cycle",
      "aggregation": "min",
      "sources": [
        "accessibility_qa_reviewer",
        "performance_auditor_persona"
      ]
    },
    "cycle_limits": {
      "max_iterations": 5,
      "respect_persona_retry_policies": true
    },
    "cycle_fallback": {
      "on_exhausted": {
        "status": "failed",
        "route_to_step": "step_9_release_coordination",
        "note": "Mark release as blocked and require human validation before proceeding."
      }
    },
    "conditional_branches": [
      {
        "condition": "accessibility_score < 0.9",
        "target_step": "step_6_accessibility_qa",
        "description": "Re-run accessibility and QA checks."
      },
      {
        "condition": "lighthouse_score < 90",
        "target_step": "step_7_performance_audit",
        "description": "Re-run performance audit and optimization."
      }
    ],
    "post_cycle": {
      "handoff_step": "step_9_release_coordination",
      "expected_inputs": [
        "accessibility_qa_reviewer.accessibility_qa_report",
        "performance_auditor_persona.performance_audit_report",
        "devops_engineer_persona.deployment_summary"
      ]
    },
    "execution_guidance": "Run architecture and design personas in parallel during planning, proceed sequentially through backend, frontend, and DevOps automation. Enter QA-performance cycle until thresholds pass. Aggregate metrics via min confidence rule; then hand off results to content and coordinator personas for release readiness validation and telemetry collection."
  },
